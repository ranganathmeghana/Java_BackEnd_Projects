<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id="Parentxml" class="Parent">
    <property name="message1" value="Hi first parent"></property>
    
    <property name="message3" value="Hi third parent"></property>
    </bean>
    
    <bean id="childxml" class="Child" parent="Parentxml" >
    <property name="message1" value="Hi first child"></property>
    
    <property name="message2" value="Hi second child"></property>
    </bean>
    </beans>